{   
  "name": "UnobtrusivePasswords",
  "manifest_version": 2,
  "version": "1.2",
  "description": "A simple master password tool that doesn't intrude on your web browsing.",
  "homepage_url": "https://github.com/cwacek/UnobtrusivePasswords",
  "browser_action": {     
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  } ,
  "permissions": [
    "storage"
  ]
} 

